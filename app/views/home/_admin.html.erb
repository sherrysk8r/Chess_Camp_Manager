<div class="row">
	<dl class="tabs" data-tab >
	  <dd class="active"><%= link_to "Upcoming Camps", home_path(anchor: "admin-1") %></dd>
	  <dd><%= link_to "Families", home_path(anchor: "admin-2") %></dd>
	  <dd><%= link_to "Instructors", home_path(anchor: "admin-3") %></dd>
	  <dd><%= link_to "Annual Camp Reports", home_path(anchor: "admin-4") %></dd>
	  <dd><%= link_to "My Camps", home_path(anchor: "admin-5") %></dd>
	  <dd><%= link_to "My Profile", home_path(anchor: "admin-6") %></dd>
	</dl>
	<div class="tabs-content">
	  <div class="content active" id="admin-1">
