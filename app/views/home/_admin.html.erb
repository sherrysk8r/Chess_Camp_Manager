	  </div>
	  <div class="content" id="admin-2">
	    <%= render partial: "tab_families" %>
	  </div>
	  <div class="content" id="admin-3">
	  	<%= render partial: "tab_instructors" %>
	  </div>
	  <div class="content" id="admin-4">
	    <%= render partial: "annual_camp_reports" %>
	  </div>
	  <div class="content" id="admin-5">
	    <%= render partial: "camps_instructing" %>
	  </div>
	  <div class="content" id="admin-6">
	    <%= render partial: "instructor_profile" %>
	  </div>
	</div>
</div>

